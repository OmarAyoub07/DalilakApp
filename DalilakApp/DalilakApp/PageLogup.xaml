<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DalilakApp.PageLogup">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout>
                <Label Text="Name"/>
                <Entry x:Name="name_u"></Entry>
            </StackLayout>
            <StackLayout>
                <Label Text="Email"/>
                <Entry x:Name="email_u"></Entry>
            </StackLayout>
            <Button x:Name="btn_logup" Text="Log up" Clicked="btn_logup_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>